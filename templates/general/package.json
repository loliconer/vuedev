    "rules": {
      "no-console": "off",
      "no-undef": "off",
      "vue/require-v-for-key": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "Chrome >= 70",
    "Firefox >= 65",
    "Safari >= 11"
  ]
